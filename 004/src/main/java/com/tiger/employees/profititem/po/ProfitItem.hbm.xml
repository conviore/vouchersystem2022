<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tiger.employees.profititem.po.ProfitItem" table="profit_item" catalog="employees">
        <id name="id" type="string">
            <column name="id" length="64"/>
        </id>
        <property name="profitTemplate" type="string">
            <column name="profit_template" length="200" />
        </property>
        <property name="itemName" type="string">
            <column name="item_name" length="100" />
        </property>
        <property name="relatedCode" type="string">
            <column name="related_code" length="100" />
        </property>
        <property name="ifFomula" type="string">
            <column name="if_fomula" length="50" />
        </property>
        <property name="formula" type="string">
            <column name="formula" length="200" />
        </property>
        <property name="comment" type="string">
            <column name="comment" length="300" />
        </property>
        <property name="itemType" type="string">
            <column name="item_type" length="50" />
        </property>
        <property name="itemNo" type="string">
            <column name="item_no" length="50" />
        </property>
    </class>
</hibernate-mapping>
